{% extends 'base.html.twig' %}


{% block body %}
    {{ form_start(form) }}
        {% if form.email is defined %}
            {{ form_row(form.email) }}
        {% endif %}

        {% if form.code is defined %}
            {{ form_row(form.code) }}
        {% endif %}

        {{ form_widget(form) }}
        
    {{ form_end(form) }}
{% endblock %}
